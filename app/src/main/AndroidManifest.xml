    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CategorieMonstres"/>

        <activity
            android:name=".FicheMonstre"
            android:label="FIRE LION" />
        <activity
            android:name=".FicheMonstrePrincipale"
            android:label="FIRE LION"/>
        <activity
            android:name=".fiche_lightspirit"
            android:label="LIGHT SPIRIT" />
        <activity
            android:name=".fiche_panda"
            android:label="PANDA" />
        <activity
            android:name=".fiche_eagle"
            android:label="THUNDER EAGLE" />
        <activity
            android:name=".fiche_turtle"
            android:label="TURTLE" />
        <activity
            android:name=".fiche_godzilla"
            android:label="GODZILLA" />
        <activity
            android:name=".fiche_metal"
            android:label="METAL SAURN" />
        <activity
            android:name=".fiche_genie"
            android:label="GENIE" />
        <activity
            android:name=".ListChooseMonsters"
            android:label="Monsters" />
        <activity
            android:name=".gold_farming"
            android:label="Gold Farming Guide"
            android:parentActivityName=".CategorieMonstres"
            android:noHistory="true">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".MonsterArenaGuide"
            android:label="Monster Arena Guide"
            android:parentActivityName=".CategorieMonstres"
            android:noHistory="true">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".breeding_epic"
            android:label="Breading Guide - Epic Monsters"
            android:parentActivityName=".CategorieMonstres"
            android:noHistory="true">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".Food"
            android:label="Food Guide"
            android:parentActivityName=".CategorieMonstres"
            android:noHistory="true">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".elements"
            android:label="Elements Guide"
            android:parentActivityName=".CategorieMonstres"
            android:noHistory="true">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
    </application>

</manifest>