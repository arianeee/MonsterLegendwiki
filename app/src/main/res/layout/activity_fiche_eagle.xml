    </android.support.constraint.ConstraintLayout>

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".FicheMonstrePrincipale">

    </android.support.constraint.ConstraintLayout>
</android.support.constraint.ConstraintLayout>